{
    "name": "pureware/template-generator",
    "description": "PHP based file generator with twig templates",
    "type": "twig",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/finder": "~5.4.0",
        "illuminate/collections": "^9.30|^8.83",
        "symfony/filesystem": "~5.4.0",
        "twig/string-extra": "~3.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0|^5.0",
        "rector/rector": "^0.14.8"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pureware\\TemplateGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Peter Roj",
            "email": "mail@peterroj.de"
        }
    ]
}
